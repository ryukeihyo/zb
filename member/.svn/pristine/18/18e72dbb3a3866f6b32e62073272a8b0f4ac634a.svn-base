

<block name="main">

                    
                    <table cellpadding="0" cellspacing="0"  border="1">
                    	<tr>
                        	<th>序号</th>
                            <th>用户编号</th>
                            <th>用户名称</th>
                            <th>申请金额</th>
                            <th>申请时间</th>
                            <th>状态</th>
                          
                        </tr>
                         <volist name="volist" id="vo" empty="$empty">
						 
						 <tr>
                        	<td>{$i}</td>
                        	<td>{$vo.username}</td>
                            <td>{$vo.mingcheng}</td>
                            <td>{$vo.money}</td>
                            <td>{$vo.addtime|time_format='Y-m-d'}</td>
                            <td><if condition="$vo[status] eq 1 ">已处理<else />未处理</if></td>
                           
                        </tr>

                        </volist>

                        
                       
                    </table>


</block>
